! Bold, Italic, Underline
*.colorBD:     #e6e1dc
!*.colorIT:
!*.colorUL:


! xft 
   Xft.dpi: 133
! rxvt_unicode
   URxvt.internalBorder: 20
   
   URxvt.scrollBar: false	
   URxvt.letterSpace:	-2

   URxvt.cursorColor:	#ffd24a
   URxvt.cursorBlink:	true

   urxvt.font:xft:monospace:bold:size=10
!   urxvt.font:xft:monospace:size=10
!   urxvt.boldfont: xft:monospace:bold:size=10
!   URxvt.italicFont: xft:Monospace:Italic:size=10

   URxvt.transparent: false
   URxvt*depth: 32
   URxvt*background: rgba:0000/0000/0200/c900
   URxvt.foreground: #f6f6ee
   URxvt.throughColor: #8080f0
   URxvt.highlightColor: #343434
   
   URxvt.saveLines: 1000000

   URxvt*fading: 0
   xterm*VT100.geometry: 90x30

! rofi   
   rofi.color-window: argb:99111111, #ff111111, #99111111
   rofi.color-normal: argb:99111111, #eeeeee, argb:99121212, #eeeeee, #121212
   rofi.color-urgent: argb:99121212, #eeeeee, argb:99121212, #121212, #121212
   rofi.color-active: argb:99111111, #eeeeee, argb:99121212, #eeeeee, #eeeeee
   rofi.width: 40 
   rofi.lines: 20
   rofi.columns: 1 
   rofi.fullscreen: false
   rofi.font: xft:Monospace 14
   rofi.bw: 0
   rofi.padding: 70
   rofi.eh: 1
   rofi.hide-scrollbar: true


