*.color8:      #323232
!
! Red
*.color1:      #da4939
*.color9:      #ff7b6b
!
! Green
*.color2:      #519f50
*.color10:     #83d182
!
! Yellow
*.color3:      #ffd24a
*.color11:     #ffff7c
!
! Blue
*.color4:      #6d9cbe
*.color12:     #9fcef0
!
! Magenta
*.color5:      #d0d0ff
*.color13:     #ffffff
!
! Cyan
*.color6:      #6e9cbe
*.color14:     #a0cef0
